<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RetrieveApp.App">
    <Application.Resources>
        <ResourceDictionary>
            <OnPlatform x:TypeArguments="x:String" x:Key="Harlow">
                <On Platform="Android" Value="harlow.ttf#Harlow Solid Italic"/>
                <!--On Platform="UWP" Value="/Assets/OpenSans-Bold.ttf#Open Sans"/>
                <On Platform="iOS" Value="OpenSans-Bold"/!-->
            </OnPlatform>
            <OnPlatform x:TypeArguments="x:String" x:Key="Baloo">
                <On Platform="Android" Value="baloo.ttf#Baloo Tamudu"/>
            </OnPlatform>
        </ResourceDictionary>
    </Application.Resources>
</Application>