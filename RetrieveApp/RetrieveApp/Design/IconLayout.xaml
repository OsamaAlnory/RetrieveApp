<?xml version="1.0" encoding="UTF-8"?>
<StackLayout xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RetrieveApp.Design.IconLayout">
    
    <Frame Padding="0" CornerRadius="25" BackgroundColor="White" IsClippedToBounds="True"
           HorizontalOptions="StartAndExpand" VerticalOptions="Start" Margin="10,10,0,0">
        <Image x:Name="icon" Aspect="AspectFill" HeightRequest="50" WidthRequest="50"/>
    </Frame>
</StackLayout>